{
  "name": "frogbot",
  "version": "1.0.0",
  "description": "A Discord bot for doing frog stuff",
  "homepage": "https://github.com/acroos/frogbot#readme",
  "bugs": {
    "url": "https://github.com/acroos/frogbot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acroos/frogbot.git"
  },
  "license": "Unlicense",
  "author": "acroos",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "start": "tsx src/server.ts",
    "register": "tsx src/commands.ts",
    "dev": "nodemon --exec tsx src/server.ts",
    "format": "prettier --write src/**/*.{ts,json,md}",
    "lint": "eslint 'src/**/*.ts'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "discord-interactions": "^4.3.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "node-cron": "^4.2.1",
    "redis": "^5.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
